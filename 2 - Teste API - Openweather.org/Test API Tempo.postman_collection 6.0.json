	"item": [
		{
			"name": "1 - Teste API - RJ",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const resposta = pm.response.json();\r",
							"\r",
							"pm.test(\"Status do Código de Retorno é 200?\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Há informações de Temperatura?\", function (){\r",
							"    pm.expect(pm.response.text()).to.include(\"temp\");\r",
							"\r",
							"});\r",
							"\r",
							"pm.test(\"Há informações de Temperatura Mínima e Máxima?\", function (){\r",
							"    pm.expect(pm.response.text()).to.include(\"temp_min\");\r",
							"    pm.expect(pm.response.text()).to.include(\"temp_max\");\r",
							"    \r",
							"});\r",
							"\r",
							"pm.test(\"Há informações de Humidade e Pressão Atmosférica?\", function (){\r",
							"    pm.expect(pm.response.text()).to.include(\"humidity\");\r",
							"    pm.expect(pm.response.text()).to.include(\"pressure\");\r",
							"    \r",
							"});\r",
							"\r",
							"pm.test(\"O estado correto é Rio de Janeiro?\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"Rio de Janeiro\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"E o Céu? Está limpo?\", function (){\r",
							"    pm.expect(pm.response.text()).to.include(\"céu limpo\");\r",
							"    \r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
